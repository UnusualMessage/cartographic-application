{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "identity": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/api/Auth/{**all}"
        }
      },
      "users": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/api/Users/{**all}"
        }
      },
      "notifications": {
        "ClusterId": "notifications",
        "Match": {
          "Path": "/api/Notifications/{**all}"
        }
      },
      "organizations": {
        "ClusterId": "main",
        "Match": {
          "Path": "/api/Organizations/{**all}"
        }
      },
      "posts": {
        "ClusterId": "main",
        "Match": {
          "Path": "/api/Posts/{**all}"
        }
      },
      "employees": {
        "ClusterId": "main",
        "Match": {
          "Path": "/api/Employees/{**all}"
        }
      }
    },
    "Clusters": {
      "identity": {
        "Destinations": {
          "main": {
            "Address": "https://localhost:5444"
          }
        }
      },
      "main": {
        "Destinations": {
          "main": {
            "Address": "https://localhost:5445"
          }
        }
      },
      "notifications": {
        "Destinations": {
          "main": {
            "Address": "https://localhost:50443"
          }
        }
      }
    }
  }
}
